{
  "schemaVersion": 1,
  "id": "gametestlib",
  "version": "${version}",
  "name": "GameTestLib",
  "description": "Very powerful GameTest extension to simplify testing",
  "authors": [
    "FX - PR0CESS"
  ],
  "contact": {
    "repo": "https://github.com/fxemo/GameTestLib"
  },
  "license": "MIT",
  "icon": "assets/gametestlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "ca.fxco.gametestlib.GameTestLib"
    ],
    "fabric-datagen": [
      "ca.fxco.gametestlib.datagen.GameTestDataGen"
    ],
    "fabric-gametest": [
      "ca.fxco.gametestlib.gametest.TestGenerator"
    ]
  },
  "accessWidener" : "gametestlib.accesswidener",
  "mixins": [
    "gametestlib.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": ">=0.76.0",
    "minecraft": "${minecraft_version}"
  }
}
